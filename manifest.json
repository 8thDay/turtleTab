{
    "manifest_version": 2,
    "name": "turtleTab",
    "description": "Momentum clone, for Chingu Voyage Cohort build to learn project",
    "version": "0.1.3",
    "permissions" : [
        "management",
        "geolocation",
        "https://api.darksky.net/*",
        "*://api.forismatic.com/*",
        "storage",
        "tabs"
    ],
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "background": {
      "persistent": false,
      "scripts": [ "js/background.js" ]
   },
    "browser_action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        },
        "default_title": "turtleTab"
    }
 }
